<script src="angular/home.js"></script>
<div class="container" ng-controller="homeCtrl">
    <h1>Welcome to the Home Page</h1>

    <form name="testForm" class="form-horizontal">
        <div class="form-group">
            <label for="email" class="col-xs-1 text-right">
                email:
            </label>
            <div class="col-xs-4">
                <input class="form-control" type="text" ng-model="user.email">
            </div>

        </div>
        <div class="form-group">
            <label for="password" class="col-xs-1 text-right">
                password:
            </label>
            <div class="col-xs-4">
                <input class="form-control" type="text" ng-model="user.password">
            </div>
        </div>
        <div class="col-xs-5 text-center">
            <button class="btn btn-primary" ng-click="submit()">提交</button>
        </div>


    </form>
    <!--<img src="img/logo.jpg" alt="">-->
</div>
